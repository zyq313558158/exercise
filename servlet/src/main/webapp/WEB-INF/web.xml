<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <context-param>
        <param-name>德玛</param-name>
        <param-value>盖伦</param-value>
    </context-param>
    <context-param>
        <param-name>轮子妈</param-name>
        <param-value>战争女神</param-value>
    </context-param>
    <!--1.ServletController-->
    <servlet>
        <servlet-name>ServletController</servlet-name>
        <servlet-class>ServletController</servlet-class>
        <init-param>
            <param-name>zyq</param-name>
            <param-value>2019</param-value>
        </init-param>
        <init-param>
            <param-name>year</param-name>
            <param-value>2019</param-value>
        </init-param>
    </servlet>
    <!--3.HttpServletController-->
    <servlet>
        <servlet-name>HttpServletController</servlet-name>
        <servlet-class>HttpServletController</servlet-class>
        <!-- <init-param>
             <param-name>zyq</param-name>
             <param-value>2019</param-value>
         </init-param>-->
        <init-param>
            <param-name>year</param-name>
            <param-value>2019</param-value>
        </init-param>
    </servlet>
    <!--2.GenericServletController-->
    <servlet>
        <servlet-name>GenericServletController</servlet-name>
        <servlet-class>GenericServletController</servlet-class>
        <init-param>
            <param-name>zyq</param-name>
            <param-value>2019</param-value>
        </init-param>
        <init-param>
            <param-name>year</param-name>
            <param-value>2019</param-value>
        </init-param>
    </servlet>

    <!--4.dispatcherServlet-->
    <servlet>
        <servlet-name>dispatcherServlet</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    </servlet>


    <!--1.ServletController-->
    <servlet-mapping>
        <servlet-name>ServletController</servlet-name>
        <url-pattern>/servlet/ServletController</url-pattern>
    </servlet-mapping>
    <!--2.GenericServletController-->
    <servlet-mapping>
        <servlet-name>GenericServletController</servlet-name>
        <url-pattern>/servlet/GenericServletController</url-pattern>
    </servlet-mapping>
    <!--3.HttpServletController-->
    <servlet-mapping>
        <servlet-name>HttpServletController</servlet-name>
        <url-pattern>/servlet/HttpServletController</url-pattern>
    </servlet-mapping>

    <!--4.dispatcherServlet-->
    <servlet-mapping>
        <servlet-name>dispatcherServlet</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
</web-app>
